{
    "manifest_version": 2,
    
    "name": "GoTo Youtube",
    "description": "Jump in youtube videos to specific time",
    "version": "1.0",

    "browser_action": {
        "default_popup": "src/popup.html"
    },

    "permissions": [
        "activeTab",
        "tabs"
    ],

    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["src/js/main.js"]
        }
    ]
}