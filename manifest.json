{
    "manifest_version": 2,
    
    "name": "GoTo Youtube",
    "description": "Jump in youtube videos to specific time",
    "version": "1.0",

    "browser_action": {
        "default_popup": "src/popup.html"
    },
    
    "background": {
        "scripts": ["src/js/background.js"]
    },

    "permissions": [
        "activeTab",
        "tabs",
        "https://www.youtube.com/watch*"
    ]
}